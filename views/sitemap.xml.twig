<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for url in urls %}
        <url>
            <loc>{{ url }}</loc>
            {% if sitemap.changeFreq is defined %}
                <changefreq>{{ sitemap.changeFreq }}</changefreq>
            {% endif %}
        </url>
    {% endfor %}
</urlset>
